{
  "name": "eventbridge-router",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f consumer --tail",
    "info": "serverless info"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.155",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^13.3.3",
    "serverless-prune-plugin": "^2.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.902.0",
    "@aws-sdk/client-eventbridge": "^3.901.0",
    "@aws-sdk/client-sqs": "^3.901.0",
    "@aws-sdk/lib-dynamodb": "^3.902.0"
  }
}
